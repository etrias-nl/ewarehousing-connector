{
    "name": "etrias/ewarehousing-connector",
    "description": "PHP connector for ewarehousing api",
    "require": {
        "php": ">=5.5",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/cache": "^3.4 || ^4.4 || ^5.0",
        "jms/serializer": "^1.8 || ^2.0 || ^3.0",
        "symfony/yaml": "^3.4 || ^4.4 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Etrias\\EwarehousingConnector\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Etrias\\EwarehousingConnector\\": "./tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.4",
        "phpunit/phpunit": "^6.2",
        "ramsey/uuid": "^3.7",
        "doctrine/cache": "^1.6"
    },
    "suggest": {
        "doctrine/cache": "Speed up jms, because it can cache metadata"
    },
    "scripts": {
        "fix-style": [
            "vendor/bin/php-cs-fixer fix --diff --show-progress run-in"
        ]
    },
    "authors": [
        {
            "name": "Christ-Jan",
            "email": "christ-jan@etrias.nl"
        }
    ],
    "minimum-stability": "stable"
}
